    Exit
EndIf
#Region ; GUI START
$Form1_1 = GUICreate("Form1", (@DesktopWidth-70), @DesktopHeight, 0, 0, $WS_SYSMENU, BitOR($WS_EX_TOPMOST, $WS_EX_TOOLWINDOW))
WinSetTrans($Form1_1, "", 215)
$FirstName = GUICtrlCreateInput("Josh", 368, 80, 137, 37)
GUICtrlSetFont(-1, 18, 400, 0, "MS Sans Serif")
$LastName = GUICtrlCreateInput("Pelak", 667, 80, 145, 37)
GUICtrlSetFont(-1, 18, 400, 0, "MS Sans Serif")
$DARA_1_user = GUICtrlCreateInput("WV171859", 515, 144, 297, 37)
GUICtrlSetFont(-1, 18, 400, 0, "MS Sans Serif")
$DARA_1_pass = GUICtrlCreateInput("Soccer1234", 515, 192, 297, 37, BitOR($GUI_SS_DEFAULT_INPUT,$ES_PASSWORD))
GUICtrlSetFont(-1, 18, 400, 0, "MS Sans Serif")
$Checkbox1 = GUICtrlCreateCheckbox("Update Password?", 817, 192, 193, 33)
GUICtrlSetFont(-1, 14, 400, 0, "MS Sans Serif")
GUICtrlSetBkColor(-1, 0xB9D1EA)
$DARA_2_user = GUICtrlCreateInput("VG639336", 515, 240, 297, 37)
GUICtrlSetFont(-1, 18, 400, 0, "MS Sans Serif")
$DARA_2_pass = GUICtrlCreateInput("Soccer99", 515, 296, 297, 37, BitOR($GUI_SS_DEFAULT_INPUT,$ES_PASSWORD))
GUICtrlSetFont(-1, 18, 400, 0, "MS Sans Serif")
$Checkbox2 = GUICtrlCreateCheckbox("Update Password?", 817, 297, 193, 33)
GUICtrlSetFont(-1, 14, 400, 0, "MS Sans Serif")
GUICtrlSetBkColor(-1, 0xB9D1EA)
$Submit = GUICtrlCreateButton("LOGON", 240, 344, 1073, 97)
GUICtrlSetFont(-1, 24, 800, 0, "MS Sans Serif")
GUICtrlSetBkColor(-1, 0xB9D1EA)
